   "source": [
    "font = cv2.FONT_HERSHEY_COMPLEX\n",
    "img = cv2.imread('modi.jfif')\n",
    "while True:\n",
    "    cv2.imshow('result', img)\n",
    "    cv2.rectangle(img,(100,100),(400,400),(0,0,255),5)\n",
    "    cv2.putText(img,\"Modi\",(20,20),font,1,(255,0,0),2)\n",
    "    if cv2.waitKey(1) == 10:\n",
    "        break"
   ]
